{
  "name": "rust-wasm-typescript-starter",
  "version": "1.0.0",
  "description": "A progressive learning project demonstrating Rust compiled to WebAssembly with TypeScript and Vite",
  "type": "module",
  "scripts": {
    "dev": "npm run build-wasm && vite --config vite.config.dev.ts",
    "dev:fast": "vite --config vite.config.dev.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-wasm": "./build-wasm.sh",
    "build-all": "npm run build-wasm && npm run build",
    "build-pages": "./build-for-pages.sh"
  },
  "keywords": [
    "rust",
    "webassembly",
    "wasm",
    "typescript",
    "vite",
    "frontend",
    "performance",
    "wasm-bindgen"
  ],
  "author": "Pekka Röyttä <roytta@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pekkaro/rust-wasm-typescript-starter.git"
  },
  "bugs": {
    "url": "https://github.com/pekkaro/rust-wasm-typescript-starter/issues"
  },
  "homepage": "https://github.com/pekkaro/rust-wasm-typescript-starter#readme",
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
}
